{
    "cmds-x": {
        "огл": "@look",
        "взять\\s+(\\S+)": "@get $1",
        "брос\\s+(\\S+)": "@drop $1",
        "(с|ю|з|в|сз|св|юз|юв|вв|вн)": "@walk $1",
        "сказ(?:ать)?\\s+(\\S.*)": "@say $1",
        "готова?": "Banzai! :)",
        "ха(?:\\-?ха)?": "Вы засмеялись: ха-ха-ха!"
    }
}
